page{
  background-color: #f0f0f0;
}
.clear{
  clear: both;
}
.search_home{
  width: 100%;
  height: auto;
}
.search_head{
  width: 100%;
  height: 100rpx;
  background-color: #0D6FB8;
  position: relative;
}
.search_head>image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 30rpx;
  left: 200rpx;
  z-index: 10;
}
.search_head>input{
  width: 650rpx;
  min-height: 60rpx;
  height: 60rpx;
  margin-left: 50rpx;
  border-radius: 30rpx;
  font-size: 28rpx;
  background-color: #ffffff;
  text-align: center;
  position: relative;
  top: 20rpx;
}
.search_body{
  width: 100%;
  height: auto;
}
.search_body_list{
  width: 100%;
  height: 140rpx;
  background-color: #ffffff;
  box-shadow: 0px 0px 3px #d4d4d4;
  margin-top: 20rpx;
  padding-top: 20rpx;
  position: relative;
}
.search_body_list_left{
  width: 120rpx;
  height: 120rpx;
  margin-left: 20rpx;
  float: left;
}
.search_body_list_left>image{
  width: 100%;
  height: 100%;
}
.search_body_list_center{
  width: 60%;
  height: 120rpx;
  margin-left: 20rpx;
  float: left;
}
.search_body_list_center_top{
  width: 100%;
  font-size: 28rpx;
}
.search_body_list_center_center{
  width: 100%;
  font-size: 22rpx;
  margin-top: 10rpx;
}
.search_body_list_center_bottom{
  width: 100%;
  font-size: 22rpx;
  margin-top: 10rpx;
}
.search_body_list_right{
  width: 100rpx;
  height: 120rpx;
  float: left;
  margin-left: 20rpx;
}
.search_body_list_right_top{
  font-size: 28rpx;
}
.search_body_list_right_bottom{
  font-size: 22rpx;
  color: #EC330E;
}